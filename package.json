{
  "name": "openaicodexcodeautocomplete",
  "displayName": "OpenAICodexCodeAutoComplete",
  "description": "Open AI Codex Code Auto Complete Visual Studio Code extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:openaicodex.startSession",
    "onCommand:openaicodex.changeKey",
    "onCommand:openaicodex.showSettings",
    "onCommand:openaicodex.changeSettings",
    "onCommand:openaicodex.suggestAutoComplete"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "openaicodex.startSession",
        "category": "OpenAI Codex",
        "title": "Auto Complete"
      },
      {
        "command": "openaicodex.changeKey",
        "category": "OpenAI Codex",
        "title": "Change Key"
      },
      {
        "command": "openaicodex.showSettings",
        "category": "OpenAI Codex",
        "title": "Show Settings"
      },
      {
        "command": "openaicodex.changeSettings",
        "category": "OpenAI Codex",
        "title": "Change Settings"
      },
      {
        "command": "openaicodex.suggestAutoComplete",
        "category": "OpenAI Codex",
        "title": "Suggest Auto Complete"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "ts-loader": "^9.2.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "publisher": "OpenAICodexCodeAutoComplete",
  "repository": {
    "type": "github",
    "url": "https://github.com/gittmaan/openaicodexcodeautocomplete"
  }
}
